<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mss.livesmart"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/live_smart_logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.mss.livesmart.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mss.livesmart.PhotoAlbumActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.mss.livesmart.HeartRateActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.mss.livesmart.PersonalInfoActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.mss.livesmart.HealthCenterActivity"
            android:label="@string/title_activity_health_center" >
        </activity>
    </application>

</manifest>
